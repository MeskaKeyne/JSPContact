<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>webcalcul-servlet-jsp</display-name>
<welcome-file-list>
<welcome-file>contacts.do</welcome-file>
</welcome-file-list>
    <servlet>
        <servlet-name>Pays</servlet-name>
        <servlet-class>be.ste.st.jsp.contact.contacts.ManagerCountry</servlet-class>
         <!--<init-param>
            <param-name>niveau</param-name>
            <param-value>10</param-value>
        </init-param> -->
    </servlet>

    <servlet-mapping>
        <servlet-name>Pays</servlet-name>
        <url-pattern>/pays.do</url-pattern>
    </servlet-mapping>
       <servlet>
        <servlet-name>Tag</servlet-name>
        <servlet-class>be.ste.st.jsp.contact.contacts.ManagerTag</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Tag</servlet-name>
        <url-pattern>/tag.do</url-pattern>
